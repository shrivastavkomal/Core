package in.co.polymorphism;

public class shape {
	private String color;
	private int bordetrwidth;
	public String getColor() {
		return color;
	}
	public int getBordetrwidth() {
		return bordetrwidth;
	}
	public shape(String color, int bordetrwidth) {
		super();
		this.color = color;
		this.bordetrwidth = bordetrwidth;
	}
	public double area() {
		return 0;
		
	}
}
