package in.co.overriding;

public class test_shape {
	public static void main(String[] args) {
		rectangle r = new rectangle();
		shape s= new shape();
		shape s1 = new rectangle();
		rectangle r1 = (rectangle)s1;
		r1.setBorderwidth(10);
		r1.setColor("Yellow");
		r1.setLength(20);
		r1.setWidth(30);
	}

}
